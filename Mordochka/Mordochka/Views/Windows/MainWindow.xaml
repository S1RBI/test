<Window x:Class="Mordochka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mordochka"
        mc:Ignorable="d"
        Background="White"
        Icon="/Resources/beauty_logo.png"
        MinHeight="250"
        MinWidth="400"
        Title="{Binding ElementName=FrameMain, Path=Content.Title}" Height="450" Width="800">
    <Grid>
        <Frame x:Name="FrameMain" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
